//=============================== –ú–µ—Ç–æ–¥ –æ—Ç–≤–µ—á–∞—é—â–∏–π –∑–∞ —É—Å—Ç–∞–Ω–æ–≤–∫—É —á–∞—Å–æ–≤ —Ä–µ–∞–ª—å–Ω–æ–≥–æ –≤—Ä–µ–º–µ–Ω–∏
void SetTime() {
  int plus_minus;     // –ò–Ω–∫—Ä–µ–º–µ–Ω—Ç –∏–ª–∏ –¥–µ–∫—Ä–µ–º–µ–Ω—Ç (+1 –∏–ª–∏ -1)
  String msg;             // –°–æ–æ–±—â–µ–Ω–∏–µ —Ä–µ–¥–∞–∫—Ç–∏—Ä—É–µ–º–æ–≥–æ –ø–∞—Ä–∞–º–µ—Ç—Ä–∞
  //String mdisplay0;       // –ü–µ—Ä–≤–∞—è —Å—Ç—Ä–æ–∫–∞ –¥–∏—Å–ø–ª–µ—è
  String mdisplay1;       // –í—Ç–æ—Ä–∞—è —Å—Ç—Ä–æ–∫–∞ –¥–∏—Å–ø–ª–µ—è
  String mdisplay2;       // –¢—Ä–µ—Ç—å—è —Å—Ç—Ä–æ–∫–∞ –¥–∏—Å–ø–ª–µ—è
  //String mdisplay3;       // –ß–µ—Ç–≤–µ—Ä—Ç–∞—è —Å—Ç—Ä–æ–∫–∞ –¥–∏—Å–ø–ª–µ—è
  delay(500);
  while(buttonPressed != BUTTON_MENU) {
    buttonPressed = getPressedButton();               // –°—á–∏—Ç—ã–≤–∞–µ–º –Ω–∞–∂–∞—Ç—É—é –∫–Ω–æ–ø–∫—É
    plus_minus = 0;
    switch(buttonPressed){
      case BUTTON_UP:   plus_minus = 1;  break;
      case BUTTON_DOWN: plus_minus = -1; break;
      case BUTTON_OK:
        pos++;              // –ü–µ—Ä–µ—Ö–æ–¥–∏–º –∫ —Å–ª–µ–¥—É—é—â–µ–º—É –∏–∑–º–µ–Ω—è–µ–º–æ–º—É –∑–Ω–∞—á–µ–Ω–∏—é
        if (pos>7) pos = 0; // –∏ –ø–æ –∫—Ä—É–≥—É
      break;    
    }
    switch (pos) {
      case 0:
        date += plus_minus;              // –ö—Ä—É—Ç–∏–º –∑–Ω–∞—á–µ–Ω–∏—è –¥–ª–∏—Ç–µ–ª—å–Ω–æ—Å—Ç–∏
        if (date > 31) date = 1;         // –û–≥—Ä–∞–Ω–∏—á–∏–≤–∞–µ–º –∑–Ω–∞—á–µ–Ω–∏—è
        msg = "    "; msg += "<<Set"; msg += " Date"; msg += ">>"; msg += "    "; // 20
        //msg = "    <<Set Date>>    ";
      break;
      case 1:
        month += plus_minus;             // –ö—Ä—É—Ç–∏–º –∑–Ω–∞—á–µ–Ω–∏—è –¥–ª–∏—Ç–µ–ª—å–Ω–æ—Å—Ç–∏
        if (month > 12) month = 1;       // –û–≥—Ä–∞–Ω–∏—á–∏–≤–∞–µ–º –∑–Ω–∞—á–µ–Ω–∏—è
        msg = "   "; msg += "<<Set"; msg += " Month"; msg += ">>"; msg += "    "; // 20
        //msg = "   <<Set Month>>    ";
      break;
      case 2:
        year += plus_minus;              // –ö—Ä—É—Ç–∏–º –∑–Ω–∞—á–µ–Ω–∏—è –¥–ª–∏—Ç–µ–ª—å–Ω–æ—Å—Ç–∏
        if (year > 99) year = 0;         // –û–≥—Ä–∞–Ω–∏—á–∏–≤–∞–µ–º –∑–Ω–∞—á–µ–Ω–∏—è
        msg = "    "; msg += "<<Set"; msg += " Year"; msg += ">>"; msg += "    "; // 20
        //msg = "    <<Set Year>>    ";
      break;
      case 3:
        day += plus_minus;               // –ö—Ä—É—Ç–∏–º –∑–Ω–∞—á–µ–Ω–∏—è –¥–ª–∏—Ç–µ–ª—å–Ω–æ—Å—Ç–∏
        if (day > 7) day = 1;            // –û–≥—Ä–∞–Ω–∏—á–∏–≤–∞–µ–º –∑–Ω–∞—á–µ–Ω–∏—è
        if (day < 1) day = 7;            // –û–≥—Ä–∞–Ω–∏—á–∏–≤–∞–µ–º –∑–Ω–∞—á–µ–Ω–∏—è
        msg = "    "; msg += "<<Set"; msg += " Day"; msg += ">>"; msg += "    "; // 19
        //msg = "    <<Set Day>>     ";
      break;
      case 4:
        hours += plus_minus;             // –ö—Ä—É—Ç–∏–º –∑–Ω–∞—á–µ–Ω–∏—è —á–∞—Å–æ–≤
        if (hours > 23) hours = 0;       // –û–≥—Ä–∞–Ω–∏—á–∏–≤–∞–µ–º –∑–Ω–∞—á–µ–Ω–∏—è
        msg = "   "; msg += "<<Set"; msg += " Hours"; msg += ">>"; msg += "    "; // 20
        //msg = "   <<Set Hours>>    ";
      break;
      case 5:
        minutes += plus_minus;           // –ö—Ä—É—Ç–∏–º –∑–Ω–∞—á–µ–Ω–∏—è –º–∏–Ω—É—Ç
        if (minutes > 59) minutes = 0;   // –û–≥—Ä–∞–Ω–∏—á–∏–≤–∞–µ–º –∑–Ω–∞—á–µ–Ω–∏—è
        msg = "  "; msg += "<<Set"; msg += " Minutes"; msg += ">>"; msg += "   "; // 20
        //msg = "  <<Set Minutes>>   ";
      break;
      case 6:
        seconds += plus_minus;           // –ö—Ä—É—Ç–∏–º –∑–Ω–∞—á–µ–Ω–∏—è –¥–ª–∏—Ç–µ–ª—å–Ω–æ—Å—Ç–∏
        if (seconds > 59) seconds = 0;   // –û–≥—Ä–∞–Ω–∏—á–∏–≤–∞–µ–º –∑–Ω–∞—á–µ–Ω–∏—è
        msg = "  "; msg += "<<Set"; msg += " Seconds"; msg += ">>"; msg += "   "; // 20
        //msg = "  <<Set Seconds>>   ";
      break;
      case 7:
        setRtc(seconds, minutes, hours, day, date, month, year);  // –£—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º —á–∞—Å—ã –∏ –¥–∞—Ç—É
        lcd.clear();
        lcd.setCursor(0,2);
        lcd.print("      -=Save"); lcd.print("d"); lcd.print("=- "); // lcd.print("    ");
        lcd.setCursor(0,3);
        lcd.print("      -=Save"); lcd.print("d"); lcd.print("=- "); // lcd.print("    ");
        buttonPressed = BUTTON_MENU;
        delay(2000);
        continue;
      break;   
    }
    lcd.setCursor(0,0);
    lcd.print("Set Date  / Set Time");   //20                 // –§–æ—Ä–º–∏—Ä—É–µ–º —Å—Ç—Ä–æ–∫–∏ —Å –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–µ–π –¥–ª—è –≤—ã–≤–æ–¥–∞ –Ω–∞ –¥–∏—Å–ø–ª–µ–π
    mdisplay1 = ByteToStrFormat(date, 2) + "/" + ByteToStrFormat(month, 2) + "/20" +  ByteToStrFormat(year, 2) + "  ";  //12
    mdisplay1 +=  ByteToStrFormat(hours, 2) + ":" + ByteToStrFormat(minutes, 2) + ":" + ByteToStrFormat(seconds, 2);  //8
    mdisplay2 = "Day=" + ByteToStrFormat(day, 1) + "      -=Save" + "=- ";
    lcd.setCursor(0,1);       // –í—ã–≤–æ–¥–∏–º —Å—Ç—Ä–æ–∫–∏ –Ω–∞ –¥–∏—Å–ø–ª–µ–π
    lcd.print(mdisplay1);
    lcd.setCursor(0,2);
    lcd.print(mdisplay2);
    lcd.setCursor(0,3);
    lcd.print(msg);
    //PrintDisplay(mdisplay0, mdisplay1, mdisplay2, mdisplay3); // –í—ã–≤–æ–¥–∏–º —Å—Ç—Ä–æ–∫–∏ –Ω–∞ –¥–∏—Å–ø–ª–µ–π
  } 
}
