  //----- –û–±—Ä–∞–±–∞—Ç—ã–≤–∞–µ–º –ø–µ—Ä–≤–æ–µ –∫–æ—Ä–º–ª–µ–Ω–∏–µ (—É—Ç—Ä–µ–Ω–Ω–µ–µ)
  if (hours_feed1 != 0) { // –ï—Å–ª–∏ —á–∞—Å –ø–µ—Ä–≤–æ–≥–æ –∫–æ—Ä–º–ª–µ–Ω–∏—è —Ä–∞–≤–µ–Ω –Ω—É–ª—é, —Ç–æ –∫–æ—Ä–º–ª–µ–Ω–∏—è –Ω–µ –±—É–¥–µ—Ç (–≤—ã–∫–ª)
    if (FEEDING1) {    
      duration = (hours-hours_feed1)*hr + (minutes-minutes_feed1)*mn + seconds;
      if (duration > (duration_feed1*mn)) {  // –µ—Å–ª–∏ —Ç–µ–∫—É—â–∞—è—è –¥–ª–∏—Ç–µ–ª—å–Ω–æ—Å—Ç—å –±–æ–ª—å—à–µ –∑–∞–¥–∞–Ω–Ω–æ–π, —Ç–æ
        FEEDING1 = false;     // –ö–æ—Ä–º–ª–µ–Ω–∏–µ –∑–∞–∫–æ–Ω—á–∏–ª–æ—Å—å
        setFILTER(ON);        // –≤–∫–ª—é—á–∞–µ–º —Ñ–∏–ª—å—Ç—Ä
      }
    }
    else {
      if ((hours == hours_feed1)&&(minutes == minutes_feed1)) { 
        FEEDING1 = true;      // –ï—Å–ª–∏ –≤—Ä–µ–º—è —Å–æ–æ—Ç–≤–µ—Ç—Å—Ç–≤—É–µ—Ç –Ω–∞—á–∞–ª—É –∫–æ—Ä–º–ª–µ–Ω–∏—è, —Ç–æ —É—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º —Ñ–ª–∞–≥ –∫–æ—Ä–º–ª–µ–Ω–∏—è,
        setFILTER(OFF);       // –≤—ã–∫–ª—é—á–∞–µ–º —Ñ–∏–ª—å—Ç—Ä
        delay(30000);         // –∏ –∂–¥–µ–º 30 —Å–µ–∫—É–Ω–¥, –ø–æ–∫–∞ –≤–æ–¥–∞ –≤ –∞–∫–≤–∞—Ä–∏—É–º–µ —É—Å–ø–æ–∫–æ–∏—Ç—Å—è
        feeding();            // –∏ –∑–∞—Ç–µ–º –ø–æ–¥–∞–µ–º –∫–æ—Ä–º —Ä—ã–±–∞–º.
      }
    }
  }
  //----- –û–±—Ä–∞–±–∞—Ç—ã–≤–∞–µ–º –≤—Ç–æ—Ä–æ–µ –∫–æ—Ä–º–ª–µ–Ω–∏–µ (–≤–µ—á–µ—Ä–Ω–µ–µ)
  if (hours_feed2 != 0) { // –ï—Å–ª–∏ —á–∞—Å –≤—Ç–æ—Ä–æ–≥–æ –∫–æ—Ä–º–ª–µ–Ω–∏—è —Ä–∞–≤–µ–Ω –Ω—É–ª—é, —Ç–æ –∫–æ—Ä–º–ª–µ–Ω–∏—è –Ω–µ –±—É–¥–µ—Ç (–≤—ã–∫–ª)
    if (FEEDING2) {    
      duration = (hours-hours_feed2)*hr + (minutes-minutes_feed2)*mn + seconds;
      if (duration > (duration_feed2*mn)) {  // –µ—Å–ª–∏ —Ç–µ–∫—É—â–∞—è—è –¥–ª–∏—Ç–µ–ª—å–Ω–æ—Å—Ç—å –±–æ–ª—å—à–µ –∑–∞–¥–∞–Ω–Ω–æ–π, —Ç–æ
        FEEDING2 = false;     // –ö–æ—Ä–º–ª–µ–Ω–∏–µ –∑–∞–∫–æ–Ω—á–∏–ª–æ—Å—å
        setFILTER(ON);        // –≤–∫–ª—é—á–∞–µ–º —Ñ–∏–ª—å—Ç—Ä
      }
    }
    else {
      if ((hours == hours_feed2)&&(minutes == minutes_feed2)) { 
        FEEDING2 = true;      // –ï—Å–ª–∏ –≤—Ä–µ–º—è —Å–æ–æ—Ç–≤–µ—Ç—Å—Ç–≤—É–µ—Ç –Ω–∞—á–∞–ª—É –∫–æ—Ä–º–ª–µ–Ω–∏—è, —Ç–æ —É—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º —Ñ–ª–∞–≥ –∫–æ—Ä–º–ª–µ–Ω–∏—è,
        setFILTER(OFF);       // –≤—ã–∫–ª—é—á–∞–µ–º —Ñ–∏–ª—å—Ç—Ä
        delay(30000);         // –∏ –∂–¥–µ–º 30 —Å–µ–∫—É–Ω–¥, –ø–æ–∫–∞ –≤–æ–¥–∞ –≤ –∞–∫–≤–∞—Ä–∏—É–º–µ —É—Å–ø–æ–∫–æ–∏—Ç—Å—è
        feeding();            // –∏ –∑–∞—Ç–µ–º –ø–æ–¥–∞–µ–º –∫–æ—Ä–º —Ä—ã–±–∞–º.
      }
    }
  }
}
